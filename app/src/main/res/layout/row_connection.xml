<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"

    >
    <RelativeLayout
        android:id="@+id/rlCon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/header"
        android:padding="@dimen/dimension5dp"
      >

        <com.mindyourelders.MyHealthCareWishes.customview.RoundedImageView
            android:id="@+id/imgConPhoto"
            android:layout_width="@dimen/dimension65dp"
            android:layout_height="@dimen/dimension65dp"
            android:src="@drawable/ic_profile_default"
            android:background="@color/colorLightBlue"
            android:layout_centerVertical="true"
            android:layout_margin="@dimen/dimension8dp"
            />

        <TextView
            android:id="@+id/txtConName"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dimension40dp"
            android:layout_toRightOf="@+id/imgConPhoto"
            android:text="Suzy Jon"
            android:singleLine="true"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:textSize="@dimen/dimension20dp"
            android:textStyle="bold"
            android:textColor="@color/colorBlack"
            android:layout_marginLeft="@dimen/dimension15dp"
            />

        <TextView
            android:id="@+id/txtConRelation"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dimension40dp"
            android:layout_toRightOf="@id/txtConName"
            android:text="Mother"
            android:layout_toLeftOf="@+id/imgForword"
            android:textSize="@dimen/dimension16dp"
            android:textColor="@color/colorBlack"
            android:layout_marginLeft="@dimen/dimension12dp"
            android:textStyle="italic"
android:gravity="center_vertical"

            />
        <TextView
            android:id="@+id/txtAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/dimension2dp"
            android:text="#203,10 Downing Street, los Angeles, California."
            android:textColor="@color/colorDarkGray"
            android:textSize="@dimen/dimension16dp"
            android:layout_toLeftOf="@+id/imglink"
            android:layout_below="@+id/txtConRelation"

            android:drawableLeft="@drawable/markers"
            android:gravity="top|left"
            android:drawablePadding="@dimen/dimension10dp"
            android:layout_toRightOf="@id/imgConPhoto"
            />
        <ImageView
            android:id="@+id/imglink"
            android:layout_width="@dimen/dimension35dp"
            android:layout_height="@dimen/dimension35dp"
            android:padding="@dimen/dimension4dp"
            android:src="@drawable/ic_insert_link_black"
            android:layout_margin="@dimen/dimension4dp"
            android:layout_toLeftOf="@+id/imgForword"
            android:layout_centerVertical="true"
            android:alpha="0.60"
            />
        <ImageView
            android:id="@+id/imgForword"
            android:layout_width="@dimen/dimension35dp"
            android:layout_height="@dimen/dimension35dp"
            android:layout_centerVertical="true"
            android:padding="@dimen/dimension4dp"
            android:src="@drawable/forword_arrow"
            android:layout_alignParentRight="true"
            android:layout_margin="@dimen/dimension4dp"
            />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        android:layout_alignParentBottom="true"
        ></View>

</RelativeLayout>
